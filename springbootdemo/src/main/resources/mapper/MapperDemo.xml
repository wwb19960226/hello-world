<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bo.mapper.MapperDemo">

    <resultMap id="student" type="com.bo.pojo.Student">
        <result property="name" column="f_name"></result>
    </resultMap>

    <select id="queryStudents" resultMap="student">
        select
        f_name
        from t_student;
    </select>
</mapper>